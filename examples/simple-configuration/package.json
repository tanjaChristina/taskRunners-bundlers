{
  "name": "simple-configuration",
  "version": "1.0.0",
  "description": "webpack demonstration without explicit configuration",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack --mode production",
    "start": "webpack --mode development",
    "build:own": "webpack --mode production --config myConfig.js",
    "start:own": "webpack --mode development --config myConfig.js"
  },
